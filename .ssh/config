# also always run chmod 600 ~/.ssh/id_rsa_personal 
# also always run chmod 600 ~/.ssh/id_rsa_work
# Personal GitHub
Host github.com-personal
  HostName github.com
  User git
  IdentityFile ~/.ssh/id_rsa_personal

# Work GitHub
Host github.com-work
  HostName github.com
  User git
  IdentityFile ~/.ssh/id_rsa
